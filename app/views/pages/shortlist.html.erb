<div class="container-content">
  <div class="container-index">
    <div class="cards-grid">
      <% @providers.each do |provider| %>
        <%= link_to provider_path(provider) do  %>
          <%= render "providers/card", provider: provider, user: @user %>
        <% end %>
      <% end %>
    </div>
  </div>
  <%= render "shared/map" %>
</div>




